{
  "name": "alesdotio.github.io",
  "version": "1.0.0",
  "description": "http://ales.io",
  "main": "index.js",
  "scripts": {
    "build:styles": "postcss src/styles/styles.scss -o _site/styles.css",
    "watch:styles": "postcss src/styles/styles.scss -o _site/styles.css --watch",
    "build:eleventy": "eleventy",
    "watch:eleventy": "eleventy --serve",
    "start:server": "node server.js",
    "build": "npm-run-all --sequential build:styles build:eleventy",
    "start": "npm-run-all --parallel watch:styles watch:eleventy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.6",
    "tailwindcss": "^3.3.2"
  }
}
